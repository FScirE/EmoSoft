{
  "name": "emoide",
  "displayName": "emoide",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "emoide",
      "properties": {
        "emoide.notifications": {
          "type":"boolean",
          "description": "Allow Notifications",
          "default": true
        }
      }
    },
    "commands": [
      {
        "title": "Start UI",
        "command": "start.ui"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@neurosity/sdk": "^6.5.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "chart.js": "^4.4.2",
    "child_process": "^1.0.2",
    "dotenv": "^16.4.5",
    "net": "^1.0.2",
    "openai": "^4.28.4",
    "vscode": "^1.1.34",
    "ws": "^8.16.0",
    "xmldom": "^0.6.0"
  }
}
